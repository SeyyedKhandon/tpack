{
  "name": "tpack",
  "version": "1.0.0",
  "publisher": "SeyyedKhandon",
  "author": "SeyyedMahdi Hassanpour",
  "displayName": "One Dark ++ (TPack)",
  "description": "Theme Essentials Extension Pack(One Dark Pro,Material Icon, FiraCode font) for Visual Studio Code",
  "license": "MIT",
  "galleryBanner": {
    "color": "#47B784",
    "theme": "dark"
  },
  "icon": "tpack.jpg",
  "activationEvents": [
    "onCommand:tpack.updateConfig"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tpack.updateConfig",
        "title": "Update Theme Pack Extension (TPack) Config"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "extensionPack": [
    "zhuangtongfa.material-theme",
    "pkief.material-icon-theme"
  ],
  "repository": {
    "url": "https://github.com/SeyyedKhandon/tpack.git"
  },
  "homepage": "https://github.com/SeyyedKhandon/tpack.git",
  "bugs": {
    "url": "https://github.com/SeyyedKhandon/tpack.git/issues"
  },
  "keywords": [
    "Theme Extension Pack",
    "One Dark Pro",
    "Material Icon Pack",
    "Fira Code font",
    "web",
    "developer experience"
  ],
  "categories": [
    "Extension Packs"
  ],
  "engines": {
    "vscode": "^1.55.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.55.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.19.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
